{
	"name": "blog-int3ractive",
	"version": "2.0.0",
	"description": "A simple Eleventy starter kit to help you have a blog of your own",
	"dependencies": {
		"@11ty/eleventy": "^2.0.1",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"@11ty/eleventy-upgrade-help": "^2.0.5",
		"canvas": "^2.6.1",
		"fast-average-color": "^9.4.0",
		"html-minifier": "^4.0.0",
		"jsdom": "^23.0.1",
		"markdown-it-attrs": "^4.1.3",
		"markdown-it-title-extract": "^4.0.0",
		"sass": "^1.49.4",
		"semver": "^7.3.2",
		"slugify": "^1.4.6"
	},
	"devDependencies": {
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.2",
		"eslint": "^8.56.0"
	},
	"scripts": {
		"sass": "sass _includes/scss/global.scss _site/css/global.css --style=compressed",
		"dev": "concurrently \"npm run sass -- --watch\" \"npm run serve\"",
		"serve": "cross-env ELEVENTY_ENV=development eleventy --incremental --serve",
		"prestart": "rm -rf _site/",
		"start": "npm run dev",
		"test": "eslint .",
		"build": "npm run sass && npm test && eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/trongthanh/11ty-hylia.git"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/trongthanh/11ty-hylia/issues"
	},
	"homepage": "https://github.com/trongthanh/11ty-hylia#readme"
}
